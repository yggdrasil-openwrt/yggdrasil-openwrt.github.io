## Happy Jekylling!
#
# bundle install
# bundle exec jekyll serve

source "https://rubygems.org"

gem "minima", "~> 2.5"

gem "github-pages", group: :jekyll_plugins

group :jekyll_plugins do
  gem "jekyll-feed", "~> 0.12"
  gem "activesupport", ">= 6.0.3.1"
  gem "jekyll-theme-primer"
  gem "jekyll-sitemap"
end
